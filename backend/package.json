{
  "name": "dubs-backend",
  "description": "Simple to-do list example",
  "version": "1.0.0",
  "main": "dist/server.js",
  "license": "MIT",
  "scripts": {
    "dev": "cross-env PAYLOAD_CONFIG_PATH=src/payload.config.ts nodemon",
    "build": "run-s build:*",
    "build:server": "tsc",
    "build:payload": "cross-env PAYLOAD_CONFIG_PATH=src/payload.config.ts payload build",
    "serve": "pm2 startOrRestart ./ecosystem.config.js",
    "types": "cross-env PAYLOAD_CONFIG_PATH=src/payload.config.ts payload generate:types",
    "graphQLSchema": "PAYLOAD_CONFIG_PATH=src/payload.config.ts payload generate:graphQLSchema"
  },
  "dependencies": {
    "@mdi/js": "^7.1.96",
    "deep-equal": "^2.2.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-http-proxy": "^1.6.3",
    "leaflet": "^1.9.2",
    "npm-run-all": "^4.1.5",
    "payload": "^1.6.17",
    "pm2": "^5.2.2",
    "react": "^18.2.0"
  },
  "devDependencies": {
    "@types/deep-equal": "^1.0.1",
    "@types/express": "^4.17.9",
    "@types/express-http-proxy": "^1.6.3",
    "@types/leaflet": "^1.9.0",
    "cross-env": "^7.0.3",
    "nodemon": "^2.0.6",
    "ts-node": "^9.1.1",
    "typescript": "^4.1.3"
  }
}
