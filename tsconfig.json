{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "$public/*": ["./public/*"],
      "$src/*": ["./src/*"],
      "$types/*": ["./src/types/*"],
      "$lib/*": ["./src/lib/*"],
      "$layouts/*": ["./src/layouts/*"],
      "$pages/*": ["./src/pages/*"]
    },
    "jsx": "preserve",
    "jsxImportSource": "solid-js"
  }
}
